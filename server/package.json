{
  "name": "vmsapi",
  "version": "1.0.0",
  "description": "visitor managment backend api",
  "main": "index.js",
  "lint-staged": {
    "src/**/*.js": [
      "yarn prettier",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "yarn lint",
    "start": "pm2 start app.js",
    "prestart": "yarn docs",
    "dev": "nodemon server.js",
    "lint": "eslint **/*.js --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:fix": "yarn lint -- --fix",
    "lint:watch": "yarn lint -- --watch",
    "docs": "apidoc -e \"(node_modules|public)\" -i . -o ./apidoc",
    "postdocs": "opn apidoc/index.html",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Yogendra0Sharma/vms/tree/master/server"
  },
  "author": "Yogendra Sharma",
  "license": "ISC",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.5.3",
    "chalk": "^2.4.2",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-validation": "^1.0.2",
    "express-validator": "^5.3.1",
    "express-winston": "^3.1.0",
    "helmet": "^3.15.0",
    "http-status": "^1.3.1",
    "ioredis": "^4.6.2",
    "jsonwebtoken": "^8.4.0",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "mongoose": "^5.4.10",
    "morgan": "^1.9.1",
    "nodemailer": "^5.1.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pm2": "^3.2.9",
    "should": "^13.2.3",
    "socket.io": "^2.2.0",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "apidoc": "*",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.2.0",
    "eslint-config-airbnb-base": "^12.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-node": "5.1.1",
    "gulp": "~3.9.1",
    "gulp-eslint": "4.0.0",
    "gulp-nodemon": "^2.2.1",
    "husky": "^0.14.3",
    "mocha": "^3.3.0",
    "nodemon": "^1.18.9",
    "nyc": "^11.0.3",
    "opn-cli": "^3.1.0",
    "sinon": "^4.0.1",
    "sinon-chai": "^3.0.0",
    "supertest": "^3.0.0"
  },
  "engines": {
    "node": ">=8",
    "yarn": "*"
  }
}
